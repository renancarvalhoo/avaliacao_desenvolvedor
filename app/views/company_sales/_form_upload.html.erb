<%= form_for(:company_sale, :url=>{:action=>"import"}, :html => { :multipart => true }) do |f| %>  

  <table>
    <tr>
      <td>
        <label for="sale_file">
          Selecione o arquivo csv para upload :
        </label>
      </td>
      <td>
        <%= f.file_field :file %>
      </td>
    </tr>
    <tr>
      <td>
        <%= f.submit 'Enviar' %>
      </td>
    </tr>
  </table>

<% end %>
